{
  "name": "leihlokal_verwaltung",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=prod rollup -c",
    "build:test": "cross-env NODE_ENV=test rollup -c",
    "build:demo": "cross-env NODE_ENV=demo rollup -c",
    "build:cypress": "cross-env NODE_ENV=cypress rollup -c",
    "build:clean": "rm -rf public/*",
    "build:all": "npm run build:clean && npm run build && OUTPUT_DIR=public/demo npm run build:demo",
    "dev": "cross-env NODE_ENV=dev rollup -c -w",
    "dev:mock_all": "cross-env NODE_ENV=dev_mock_all rollup -c -w",
    "dev:mock_db": "cross-env NODE_ENV=dev_mock_db rollup -c -w",
    "dev:mock_wc": "cross-env NODE_ENV=dev_mock_wc rollup -c -w",
    "demo": "npm run build:demo && npm run start",
    "test": "npm run build:test && npm run start",
    "start": "sirv public --port 5000",
    "unit_test": "jest --env=jsdom",
    "unit_test:watch": "npm run unit_test -- --watch",
    "unit_test:debug": "node --inspect node_modules/.bin/jest --runInBand --env=jsdom",
    "integration_test_selenium": "concurrently -k --success first \"npm run test > /dev/null\" \"(wait-on http://127.0.0.1:5000/ && node ./node_modules/mocha/bin/mocha --timeout 15000 ./test/integration_selenium)\"",
    "integration_test_cypress:run": "npm run build:test && cypress run --config-file ./test/integration_cypress/config/config.json",
    "integration_test_cypress:open": "npm run build:test && cypress open --config-file ./test/integration_cypress/config/config.json",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@bahmutov/cy-rollup": "^2.0.0",
    "@beyonk/svelte-datepicker": "^12.0.0",
    "@beyonk/svelte-notifications": "^4.1.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.2.1",
    "babel-jest": "^27.4.6",
    "babel-plugin-rewire-exports": "^2.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "cl-editor": "^2.2.2",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.3.1",
    "dotenv-flow": "^3.2.0",
    "esm": "^3.2.25",
    "fa-svelte": "^3.1.0",
    "jest": "^27.1.0",
    "jest-svelte-events": "^1.0.5",
    "js-logger": "^1.6.1",
    "lru-cache": "^7.7.1",
    "patch-package": "^6.4.7",
    "mocha": "^9.2.0",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "rewire": "^5.0.0",
    "rollup": "^2.59.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.1.1",
    "simple-svelte-autocomplete": "^2.1.0",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.42.4",
    "svelte-chartjs": "^1.1.1",
    "svelte-checkbox": "^1.0.1",
    "svelte-component-double": "^0.8.0",
    "svelte-htm": "^1.1.1",
    "svelte-jester": "^2.1.5",
    "svelte-jester-mock": "^1.0.3",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-select": "^4.4.0",
    "svelte-simple-modal": "1.3.2",
    "svelte-spa-router": "^3.2.0",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.0"
  }
}
