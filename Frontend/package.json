{
  "name": "leihlokal_verwaltung",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "build:test": "cross-env NODE_ENV=test rollup -c",
    "build:demo": "cross-env NODE_ENV=test rollup -c",
    "build:dev": "rollup -c",
    "build:clean": "rm -rf public/*",
    "build:all": "npm run build:clean && npm run build && OUTPUT_DIR=public/demo npm run build:demo",
    "dev": "cross-env NODE_ENV=test rollup -c -w",
    "dev:db": "rollup -c -w",
    "demo": "npm run build:demo && npm run start",
    "start": "sirv public --port 5000",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "integration_test": "concurrently -k --success first \"npm run demo > /dev/null\" \"(wait-on http://127.0.0.1:5000/ && node ./node_modules/mocha/bin/mocha --timeout 15000 ./integration_tests)\""
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@beyonk/svelte-notifications": "^3.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^12.7.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-rewire-exports": "^2.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "esm": "^3.2.25",
    "fa-svelte": "^3.1.0",
    "jest": "^26.6.3",
    "jest-svelte-events": "^1.0.5",
    "lru-cache": "^6.0.0",
    "mocha": "^8.3.2",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "rewire": "^5.0.0",
    "rollup": "^2.40.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.0.0-beta.2",
    "simple-svelte-autocomplete": "^1.2.4",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.34.0",
    "svelte-calendar": "^2.0.4",
    "svelte-checkbox": "^1.0.1",
    "svelte-component-double": "^0.8.0",
    "svelte-htm": "^1.1.1",
    "svelte-jester": "^1.3.0",
    "svelte-jester-mock": "^1.0.3",
    "svelte-loading-spinners": "^0.1.4",
    "svelte-select": "^3.17.0",
    "svelte-simple-modal": "^0.9.0",
    "svelte-spa-router": "^3.1.0",
    "wait-on": "^5.3.0"
  }
}
